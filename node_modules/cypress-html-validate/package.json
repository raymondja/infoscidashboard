{
  "name": "cypress-html-validate",
  "version": "1.3.0",
  "description": "Cypress plugin for html-validate",
  "keywords": [
    "html",
    "html-validate",
    "cypress",
    "validator"
  ],
  "homepage": "https://gitlab.com/html-validate/cypress-html-validate",
  "bugs": {
    "url": "https://gitlab.com/html-validate/cypress-html-validate/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/html-validate/cypress-html-validate.git"
  },
  "license": "MIT",
  "author": "David Sveningsson <ext@sidvind.com>",
  "main": "dist/commands.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run || true",
    "digest": "scripts/digest",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prepare": "git config commit.template ./node_modules/@html-validate/commitlint-config/gitmessage",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "semantic-release": "semantic-release",
    "server": "node server.js",
    "start": "start-server-and-test server http://localhost:8080 cypress:open",
    "test": "start-server-and-test server http://localhost:8080 cypress:run && jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "./scripts/pre-push"
    }
  },
  "commitlint": {
    "extends": [
      "@html-validate"
    ]
  },
  "lint-staged": {
    "*.{ts,js,json,md,scss}": "prettier --write"
  },
  "prettier": "@html-validate/prettier-config",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "./tests/tsconfig.json"
      }
    },
    "preset": "@html-validate/jest-config",
    "testRegex": "(tests|src)/.*.spec.[jt]s$"
  },
  "dependencies": {
    "deepmerge": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@html-validate/commitlint-config": "1.1.1",
    "@html-validate/eslint-config": "2.0.0",
    "@html-validate/jest-config": "1.0.33",
    "@html-validate/prettier-config": "1.1.0",
    "@html-validate/semantic-release-config": "1.1.0",
    "@types/estree": "0.0.45",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "angular": "1.8.2",
    "cypress": "5.5.0",
    "eslint": "7.13.0",
    "eslint-plugin-cypress": "2.11.2",
    "express": "4.17.1",
    "html-validate": "4.0.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.1",
    "mocha": "8.2.1",
    "mocha-spec-json-output-reporter": "1.1.7",
    "prettier": "2.1.2",
    "semantic-release": "17.2.2",
    "serve-static": "1.14.1",
    "start-server-and-test": "1.11.5",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
  },
  "peerDependencies": {
    "cypress": "^3.4.0 || ^4.0.0 || ^5.0.0",
    "html-validate": "^2.17.0 || ^3.0.0 || ^4.0.0"
  },
  "engines": {
    "node": ">= 10.0"
  },
  "release": {
    "extends": "@html-validate/semantic-release-config"
  },
  "renovate": {
    "extends": [
      "@html-validate"
    ]
  }
}
